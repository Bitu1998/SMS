                    <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="col-xl-12">
                        <div class="card" id="pdfdiv">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <footer class="py-2 w-100">
        <p class="copy-right text-center mb-0">
            © 2025 , All rights reserved
        </p>
    </footer>

    @RenderSection("scripts", required: false)

    <script>
        $(document).ready(function () {
            sessionUniqueUserId = "";
        });

        var url = window.location.href;
        var l = window.location;
        var base_url = l.protocol + "//" + l.host;
        $('.navbar-nav a').each(function () {
            var newUrl = $(this).attr('href');
            newUrl = base_url + newUrl;
            if (newUrl == url) {
                $(this).addClass("active");
            }
        }).get();
    </script>

    <script>
        $("body").on("click", ".ViewPDF", function () {
            var path = $(this).attr("id");
            $("#pdfdiv").html("");
            var html = "";
            html += '<object data="' + path + '" type="application/pdf" width="100%" height="650px"></object>';
            $("#pdfdiv").append(html);
            $('#PdfViewModel').modal('show');
        });
    </script>

    <script>
        $(document).ready(function () {
            $("#toggleButton").click(function () {
                // Sidebar toggle
                $("#sidebar").toggleClass("collapsed");

                // Main content toggle
                $(".main-content").toggleClass("full-width");

                // Change the arrow direction
                if ($("#sidebar").hasClass("collapsed")) {
                    $("#toggleIcon").removeClass("bi-chevron-double-left").addClass("bi-chevron-double-right");
                } else {
                    $("#toggleIcon").removeClass("bi-chevron-double-right").addClass("bi-chevron-double-left");
                }
            });
        });


    </script>

</body>
</html>
